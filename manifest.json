{
    "name": "Do It",
    "version": "0.0.2",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Get motivated"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "icons": {
        "128": "./icons/icon128.png",
        "48": "./icons/icon48.png",
        "16": "./icons/icon16.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Ctrl+Shift+A",
                "mac": "Command+Shift+A",
                "chromeos": "Ctrl+Shift+A",
                "linux": "Ctrl+Shift+A"
            }
        }
    }
}
